{
  "name": "have-we-met",
  "version": "0.1.0",
  "description": "Identity resolution library for Node.js - match, deduplicate, and merge records across datasets with deterministic, probabilistic, and ML-based matching",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "lint": "eslint src && tsc --noEmit",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "identity",
    "resolution",
    "entity-resolution",
    "record-linkage",
    "deduplication",
    "matching",
    "merge",
    "fuzzy-matching",
    "probabilistic-matching",
    "machine-learning",
    "data-quality",
    "master-data-management",
    "mdm",
    "customer-data",
    "patient-matching"
  ],
  "author": "Matt Barrett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/8arr3tt/have-we-met.git"
  },
  "bugs": {
    "url": "https://github.com/8arr3tt/have-we-met/issues"
  },
  "homepage": "https://github.com/8arr3tt/have-we-met#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@prisma/client": "^7.3.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "typeorm": "^0.3.28",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "libphonenumber-js": "^1.12.36"
  }
}
